<?xml-stylesheet href="chrome://global/skin/global.css"?>
<?xml-stylesheet href="chrome://browser/skin/devtools/common.css"?>
<?xml-stylesheet href="chrome://browser/skin/devtools/widgets.css"?>
<?xml-stylesheet href="chrome://browser/skin/devtools/profiler.css"?>
<?xml-stylesheet href="chrome://browser/content/devtools/widgets.css"?>

<window xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">

<panel
xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
id="devtools-magnifier-panel"
noautofocus="true"
noautohide="true"
level="floating"
orient="horizontal"
style="border-radius: 4px !important; border:1px solid #111;-moz-appearance:none; background: rgb(53,67,82);">
  <vbox
  style="background: url('data:image/png;base64,');">
    <toolbar
    class="devtools-toolbar"
    style="border-bottom: 1px solid rgb(20,30,39) !important;"
    flex="1">
      <box
      class="devtools-searchinput"
      style="margin-left: 4px ! important; width: 32px; background-image: url(&quot;&quot;) ! important; box-shadow: 0px 0px 0px black ! important; background-color: rgb(222, 237, 253);"
      />
      <textbox
      class="devtools-searchinput"
      />
      <menulist
      class="devtools-menulist"
      sizetopopup="pref"
      label="RGB">
        <menupopup>
          <menuitem
          label="RGB"
          style="text-align: center"
          selected="true"
          />
          <menuitem
          label="HSL"
          style="text-align: center"
          />
          <menuitem
          label="Hex"
          style="text-align: center"
          />
        </menupopup>
        </menulist>
        <menulist
        class="devtools-menulist"
        sizetopopup="pref"
        label="Content Zoom">
          <menupopup>
            <menuitem
            label="Content Zoom"
            style="text-align: center"
            selected="true"
            />
            <menuitem
            label="Browser Zoom"
            style="text-align: center"
            />
            <menuitem
            label="Mouse Zoom"
            style="text-align: center"
            />
            <menuitem
            label="Panel Zoom"
            style="text-align: center"
            />
          </menupopup>
          </menulist>
          <spacer
          flex="1"
          />
          <toolbarbutton
          class="developer-toolbar-button"
          type="checkbox"
          style="min-height: 24px ! important; max-height: 24px ! important; min-width: 24px ! important; max-width: 24px ! important; list-style-image: url(&quot;data:image/png;base64,&quot;); -moz-image-region: rect(0px, 32px, 16px, 16px);"
          checked="true"
          />
          <toolbarbutton
          type="checkbox"
          class="developer-toolbar-button"
          style="min-height: 24px ! important; max-height: 24px ! important; min-width: 24px ! important; max-width: 24px ! important; list-style-image: url(&quot;data:image/png;base64,&quot;); -moz-image-region: rect(0px, 16px, 16px, 0px);">
            <menupopup>
              <menuitem
              label="Show Gridlines"
              type="checkbox"
              checked="false"
              />
              <menuitem
              label="Show smooth edges"
              type="checkbox"
              checked="null"
              />
            </menupopup>
            </toolbarbutton>
            <toolbarbutton
            class="devtools-closebutton"
            />
            </toolbar>
            <hbox>
              <vbox>
                <box
                style="display:block; position: relative; margin: 10px 10px 5px">
                  <canvas
                  xmlns="http://www.w3.org/1999/xhtml"
                  style="image-rendering: -moz-crisp-edges; background-color: black; width: 476px; height: 308px;"
                  width="119"
                  height="77"></canvas>
                    <box
                    style="position: absolute; top: 0px; left: 0px; width: 476px; height: 308px; background-size: 4px 4px; background-image: -moz-linear-gradient(left center , transparent 1px, transparent 1px), -moz-linear-gradient(center top , transparent 1px, transparent 1px);"
                    />
                    <box
                    style="display: block; position: absolute; border-width: 2px; border-style: solid; -moz-box-sizing: content-box; margin-left: -1px; margin-top: -1px; pointer-events: none; width: 3px; height: 3px; left: 236px; top: 152px;"
                    /></box>
                    <spacer
                    flex="1"
                    />
                    <hbox
                    style="max-height: 3px;">
                      <hbox
                      flex="1">
                        <label
                        value="2x"
                        style="padding: 0px 2px;color:#eef"
                        />
                        <box
                        flex="1"
                        style="min-height: 14px; background-repeat: no-repeat; border-bottom: 1px solid rgb(238, 238, 255); margin: 1px; max-height: 14px; background-image: -moz-linear-gradient(right center , rgb(238, 238, 255) 1px, transparent 1px); background-size: 33.3333% 100%;"
                        />
                        <label
                        value="8x"
                        style="padding: 0px 2px;color:#eef"
                        />
            </hbox>
            <box
            draggable="true"
            style="min-height:3px;max-height:3px;margin-right: 2px;min-width:3px;max-width:3px;cursor: se-resize;-moz-appearance:resizer;"
            />
            </hbox>
            </vbox>
            </hbox>
            </vbox>
            </panel>

</window>