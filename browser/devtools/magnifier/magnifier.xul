<?xml-stylesheet href="chrome://global/skin/global.css"?>
<?xml-stylesheet href="chrome://browser/skin/devtools/common.css"?>
<?xml-stylesheet href="chrome://browser/skin/devtools/widgets.css"?>
<?xml-stylesheet href="chrome://browser/skin/devtools/profiler.css"?>
<?xml-stylesheet href="chrome://browser/content/devtools/widgets.css"?>
<?xml-stylesheet href="chrome://browser/content/devtools/magnifier.css" type="text/css"?>

<window xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  <vbox id="magnifier-container" flex="1">
    <toolbar
    class="devtools-toolbar"
    style="border-bottom: 1px solid rgb(20,30,39) !important;"
    flex="1">
      <box
      class="devtools-searchinput"
      style="margin-left: 4px ! important; width: 32px; ! important; box-shadow: 0px 0px 0px black ! important; background-color: rgb(222, 237, 253);"
      ></box>
      <textbox
      class="devtools-searchinput"
      ></textbox>
      <menulist
      class="devtools-menulist"
      sizetopopup="pref"
      label="RGB">
        <menupopup>
          <menuitem
          label="RGB"
          style="text-align: center">
          selected="true"
          ></menuitem>
          <menuitem
          label="HSL"
          style="text-align: center">
          </menuitem>
          <menuitem
          label="Hex"
          style="text-align: center">
          </menuitem>
        </menupopup>
        </menulist>
        <menulist
        class="devtools-menulist"
        sizetopopup="pref"
        label="Content Zoom">
          <menupopup>
            <menuitem
            label="Content Zoom"
            style="text-align: center"
            selected="true"></menuitem>
            <menuitem
            label="Browser Zoom"
            style="text-align: center"></menuitem>
            <menuitem
            label="Mouse Zoom"
            style="text-align: center"></menuitem>
            <menuitem
            label="Panel Zoom"
            style="text-align: center"></menuitem>
          </menupopup>
          </menulist>
          <spacer
          flex="1"
          />
          <toolbarbutton
          class="developer-toolbar-button"
          type="checkbox"
          style="min-height: 24px ! important; max-height: 24px ! important; min-width: 24px ! important; max-width: 24px ! important; list-style-image: url('data:image/png;base64,'); -moz-image-region: rect(0px, 32px, 16px, 16px);"
          checked="true"
          ></toolbarbutton>
          <toolbarbutton
          type="checkbox"
          class="developer-toolbar-button"
          style="min-height: 24px ! important; max-height: 24px ! important; min-width: 24px ! important; max-width: 24px ! important; list-style-image: url('data:image/png;base64,'); -moz-image-region: rect(0px, 16px, 16px, 0px);">
            <menupopup>
              <menuitem
              label="Show Gridlines"
              type="checkbox"
              checked="false"
              ></menuitem>
              <menuitem
              label="Show smooth edges"
              type="checkbox"
              checked="null"
              ></menuitem>
            </menupopup>
            </toolbarbutton>
            <toolbarbutton
            class="devtools-closebutton"
            ></toolbarbutton>
            </toolbar>
            <hbox flex="1">
              <vbox  flex="1">
                <box
                style="display:block; position: relative; margin: 10px 10px 5px">
                  <canvas id="canvas"
                  xmlns="http://www.w3.org/1999/xhtml"
                  style="image-rendering: -moz-crisp-edges;"></canvas>
                    <box
                    style="position: absolute; top: 0px; left: 0px; background-size: 4px 4px; background-image: -moz-linear-gradient(left center , transparent 1px, transparent 1px), -moz-linear-gradient(center top , transparent 1px, transparent 1px);"
                    ></box>
                    <box
                    style="display: block; position: absolute; border-width: 2px; border-style: solid; -moz-box-sizing: content-box; margin-left: -1px; margin-top: -1px; pointer-events: none; "
                    /></box>
                    <spacer
                    flex="1"
                    />
                    <hbox
                    style="max-height: 3px;">
                      <hbox
                      flex="1">
                        <label
                        value="2x"
                        style="padding: 0px 2px;color:#eef"
                        ></label>
                        <box
                        flex="1"
                        style="min-height: 14px; background-repeat: no-repeat; border-bottom: 1px solid rgb(238, 238, 255); margin: 1px; max-height: 14px; background-image: -moz-linear-gradient(right center , rgb(238, 238, 255) 1px, transparent 1px); background-size: 33.3333% 100%;"
                        ></box>
                        <label
                        value="8x"
                        style="padding: 0px 2px;color:#eef"
                        ></label>
            </hbox>
            <box
            draggable="true"
            style="min-height:3px;max-height:3px;margin-right: 2px;min-width:3px;max-width:3px;cursor: se-resize;-moz-appearance:resizer;"
            >
          </box>
            </hbox>
            </vbox>
            </hbox>
            <box>
              <!--
              <input type="range" id="zoom-level" min="1" max="10"
                  xmlns="http://www.w3.org/1999/xhtml" />-->
              <label for="zoom-level">Zoom</label><scale id="zoom-level" min="1" max="10" flex="1"/>
            </box>
            </vbox>

</window>